<script src="player-bundle.js"></script>

<li>
  <div class="row">
      <h3><%= track.name %></h3>
  </div>

  <div class="row">
    <div class="column">
      <img src="<%= track.image %>" class="w3-round w3-margin-bottom" alt=" <%= track.name %>" >
    </div>
    <div class="barColumn">
      <img src="images/bar.png" class="w3-round w3-margin-bottom" alt=" <%= track.name %>">
      <button class="playbutton" onclick="play('<%= track.url %>')">Play</button>
      <button class="pausebutton" onclick="pause()">Pause</button>

    </div>
  </div>
</li>
